source 'https://rubygems.org'
=begin
rails_version = ENV['RAILS_VERSION'] || "default"
rails =
  case rails_version
  when 'master'
    { github: 'rails/rails' }
  when 'default'
    '>= 5.0'
  else
    "~> #{ENV['RAILS_VERSION']}"
  end
=end
hm_proj_rails = '4.1.16'
gem 'rails', hm_proj_rails

gemspec
